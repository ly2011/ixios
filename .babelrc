{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": [">1%", "last 2 versions", "safari >= 7"]
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime"
    ]
  ],
  "env": {
    "test": {
      "presets": [["@babel/preset-env", {
        "targets": {
          "browsers": [">1%", "last 2 versions", "safari >= 7"]
        }
      }]]
    }
  }
}